<% if Like.find_by(post_id: @post.id, user_id: current_user.id) %>
	$('.feeld<%=@post.id%> img').attr('src', "<%= image_path "feeldrev.png" %>")
<% else %>
	$('.feeld<%=@post.id%> img').attr('src', "<%= image_path "feeld.png" %>")
<% end %>
$('.feeldcnt<%=@post.id%>').html("x <%=@post.likes.count%>")
