$('#posts-container').html('');
<% if @isMine %>
$('#posts-container').html("<%=j render 'projects/form2' %>");
<% end %>

<% if @projects.length > 0 %>
	$('#posts-container').append("<%=j render 'projects/projects' %>");
<% else %>
	$('#posts-container').append("<%=j render 'projects/empty' %>");
<% end %>
